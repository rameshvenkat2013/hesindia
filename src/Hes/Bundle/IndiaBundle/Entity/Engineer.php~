<?php

namespace Hes\Bundle\IndiaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Engineer
 */
class Engineer
{
    /**
     * @var integer
     */
    private $enggId;

    /**
     * @var string
     */
    private $engineerName;

    /**
     * @var string
     */
    private $enggEmail;

    /**
     * @var string
     */
    private $designation;

    /**
     * @var string
     */
    private $bloodGroup;

    /**
     * @var string
     */
    private $mobileNo;

    /**
     * @var string
     */
    private $address1;

    /**
     * @var string
     */
    private $address2;

    /**
     * @var integer
     */
    private $pincode;

    /**
     * @var string
     */
    private $city;

    /**
     * @var string
     */
    private $state;

    /**
     * @var \DateTime
     */
    private $dateOfJoining;

    /**
     * @var integer
     */
    private $rowId;


    /**
     * Set enggId
     *
     * @param integer $enggId
     * @return Engineer
     */
    public function setEnggId($enggId)
    {
        $this->enggId = $enggId;

        return $this;
    }

    /**
     * Get enggId
     *
     * @return integer 
     */
    public function getEnggId()
    {
        return $this->enggId;
    }

    /**
     * Set enggEmail
     *
     * @param string $enggEmail
     * @return Engineer
     */
    public function setEnggEmail($enggEmail)
    {
        $this->enggEmail = $enggEmail;

        return $this;
    }

    /**
     * Get enggEmail
     *
     * @return string 
     */
    public function getEnggEmail()
    {
        return $this->enggEmail;
    }

    /**
     * Set engineerName
     *
     * @param string $engineerName
     * @return Engineer
     */
    public function setEngineerName($engineerName)
    {
        $this->engineerName = $engineerName;

        return $this;
    }

    /**
     * Get engineerName
     *
     * @return string 
     */
    public function getEngineerName()
    {
        return $this->engineerName;
    }

    /**
     * Set designation
     *
     * @param string $designation
     * @return Engineer
     */
    public function setDesignation($designation)
    {
        $this->designation = $designation;

        return $this;
    }

    /**
     * Get designation
     *
     * @return string 
     */
    public function getDesignation()
    {
        return $this->designation;
    }

    /**
     * Set bloodGroup
     *
     * @param string $bloodGroup
     * @return Engineer
     */
    public function setBloodGroup($bloodGroup)
    {
        $this->bloodGroup = $bloodGroup;

        return $this;
    }

    /**
     * Get bloodGroup
     *
     * @return string 
     */
    public function getBloodGroup()
    {
        return $this->bloodGroup;
    }

    /**
     * Set mobileNo
     *
     * @param string $mobileNo
     * @return Engineer
     */
    public function setMobileNo($mobileNo)
    {
        $this->mobileNo = $mobileNo;

        return $this;
    }

    /**
     * Get mobileNo
     *
     * @return string 
     */
    public function getMobileNo()
    {
        return $this->mobileNo;
    }

    /**
     * Set address1
     *
     * @param string $address1
     * @return Engineer
     */
    public function setAddress1($address1)
    {
        $this->address1 = $address1;

        return $this;
    }

    /**
     * Get address1
     *
     * @return string 
     */
    public function getAddress1()
    {
        return $this->address1;
    }

    /**
     * Set address2
     *
     * @param string $address2
     * @return Engineer
     */
    public function setAddress2($address2)
    {
        $this->address2 = $address2;

        return $this;
    }

    /**
     * Get address2
     *
     * @return string 
     */
    public function getAddress2()
    {
        return $this->address2;
    }

    /**
     * Set pincode
     *
     * @param integer $pincode
     * @return Engineer
     */
    public function setPincode($pincode)
    {
        $this->pincode = $pincode;

        return $this;
    }

    /**
     * Get pincode
     *
     * @return integer 
     */
    public function getPincode()
    {
        return $this->pincode;
    }

    /**
     * Set city
     *
     * @param string $city
     * @return Engineer
     */
    public function setCity($city)
    {
        $this->city = $city;

        return $this;
    }

    /**
     * Get city
     *
     * @return string 
     */
    public function getCity()
    {
        return $this->city;
    }

    /**
     * Set state
     *
     * @param string $state
     * @return Engineer
     */
    public function setState($state)
    {
        $this->state = $state;

        return $this;
    }

    /**
     * Get state
     *
     * @return string 
     */
    public function getState()
    {
        return $this->state;
    }

    /**
     * Set dateOfJoining
     *
     * @param \DateTime $dateOfJoining
     * @return Engineer
     */
    public function setDateOfJoining($dateOfJoining)
    {
        $this->dateOfJoining = $dateOfJoining;

        return $this;
    }

    /**
     * Get dateOfJoining
     *
     * @return \DateTime 
     */
    public function getDateOfJoining()
    {
        return $this->dateOfJoining;
    }

    /**
     * Get rowId
     *
     * @return integer 
     */
    public function getRowId()
    {
        return $this->rowId;
    }
    /**
     * @var integer
     */
    private $activeStatus;


    /**
     * Set activeStatus
     *
     * @param integer $activeStatus
     * @return Engineer
     */
    public function setActiveStatus($activeStatus)
    {
        $this->activeStatus = $activeStatus;

        return $this;
    }

    /**
     * Get activeStatus
     *
     * @return integer 
     */
    public function getActiveStatus()
    {
        return $this->activeStatus;
    }
}
