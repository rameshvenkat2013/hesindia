<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Hes\Bundle\IndiaBundle\Entity\User" table="user">
    <id name="serialId" type="integer" column="serial_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userEmail" type="string" column="user_email" length="100" nullable="false"/>
    <field name="userPassword" type="string" column="user_password" length="100" nullable="false"/>
    <field name="userRole" type="string" column="user_role" length="50" nullable="false"/>
    <field name="userLevel" type="integer" column="user_level" nullable="false"/>
  </entity>
</doctrine-mapping>
